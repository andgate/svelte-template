{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "inlineSources": false,
    "isolatedModules": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "forceConsistentCasingInFileNames": true,
    "preserveWatchOutput": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "moduleResolution": "node",
    "module": "ESNext",
    "lib": [
      "ESNext"
    ],
    "types": [
      "vitest/globals"
    ]
  },
  "exclude": [
    "dist/*",
    "build/*",
    "tmp/*",
    "node_modules/*",
    "**/*.cjs"
  ],
  "include": [
    "**/*.ts"
  ]
}